#include<string.h>
#include<sys/types.h>
#include<sys/stat.h>
#include<fcntl.h>
#include<stdio.h>
#include<unistd.h>
#include<errno.h>
#include<stdio.h>
#include<dirent.h>
int main(int argc, char const* argv[])
{
	DIR* dir = opendir("./");
	if (dir == NULL)
	{
		perror(" ");
		return  0;
	}
	struct dirent* dirp = NULL;
	while (dirp = readdir(dir))
	{
		printf("%s\n", dirp->d_name);
	}
	closedir(dir);
	return 0;
}
